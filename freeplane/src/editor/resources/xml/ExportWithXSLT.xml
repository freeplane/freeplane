<?xml version="1.0"?>
<export_with_xslt>
    <action
        name="accessories/plugins/ExportWithXSLT_HTML"
    >
        <properties
            file_type="html"
            branch_export="true"
            xslt_file="/xslt/toxhtml.xsl"
            create_dir="true"
            files_to_copy="marktree.js,minus.png,plus.png,ilink.png,treestyles.css"
            file_prefix="/html/"
            copy_icons="true"
            load_file="true"
            create_html_linked_image="false" />
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_HTML3"
    >
        <properties
            file_type="html"
            branch_export="true"
            xslt_file="/xslt/freeplane2html.xsl"
            create_dir="true"
            files_to_copy="freeplane2html.css,hide.png,leaf.png,show.png,ilink.png"
            file_prefix="/html/"
            copy_icons="true"
            load_file="true"
            create_html_linked_image="true"/>
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_Applet"
    >
        <properties
            file_type="html"
            branch_export="false"
            xslt_file="/xslt/freeplane2applet.xsl"
            create_dir="true"
            files_to_copy="${org.freeplane.core.dir.lib}freeplaneviewer.jar,
            ${org.freeplane.core.dir.lib}freeplaneapi_viewer.jar,
            ${org.freeplane.core.dir.lib}kitfox-svg-salamander-1.1.1-p1.jar,
            /html/freeplane_applet.jnlp"
            file_prefix=""
            copy_map="true"
            copymode="FILE"
            mode="FILE"
            load_file="true" 
            set_properties="show_note_icons,
            				show_icon_for_attributes,
            				use_common_out_point_for_root_node"/>
    </action>
    <action
        name="accessories/plugins/Export_JavaWebStart"
    >
        <properties
            file_type="jnlp"
            branch_export="false"
            xslt_file="/xslt/freeplane2ws.xsl"
            create_dir="true"
            files_to_copy="${org.freeplane.core.dir.lib}freeplaneviewer.jar,
            ${org.freeplane.core.dir.lib}freeplaneapi_viewer.jar,
            ${org.freeplane.core.dir.lib}kitfox-svg-salamander-1.1.1-p1.jar"
            file_prefix=""
            copy_map="true"
            copymode="FILE"
            mode="FILE"
            load_file="true" 
            set_properties="show_note_icons,
            				show_icon_for_attributes,
            				use_common_out_point_for_root_node"/>
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_TASKSTJI"
    >
        <properties
            file_type="tji"
            branch_export="true"
            name_extension=".tasks"
            xslt_file="/xslt/mm2tjitasks.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
    <action
        name="accessories/plugins/ExportWithXSLT_RESOURCESTJI"
    >
        <properties
            file_type="tji"
            branch_export="true"
            name_extension=".resources"
            xslt_file="/xslt/mm2tjiresources.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
    <action
        name="accessories/plugins/ExportWithTWiki"
    >
        <properties
            file_type="twi"
            branch_export="true"
            name_extension=""
            xslt_file="/xslt/mm2twiki.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
</export_with_xslt>
